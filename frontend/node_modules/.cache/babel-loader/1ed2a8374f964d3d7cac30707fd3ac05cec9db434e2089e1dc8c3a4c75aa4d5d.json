ction handleCancel(reservation) {\n      var _this = this;\n      this.$confirm('确定要取消该预约吗?', '提示', {\n        confirmButtonText: '确定',\n        cancelButtonText: '取消',\n        type: 'warning'\n      }).then(function () {\n        _this.$message.success(\"\\u5DF2\\u53D6\\u6D88\\u9884\\u7EA6\\uFF1A\".concat(reservation.room, \" \").concat(reservation.seat));\n        // TODO: 调用取消API\n      })[\"catch\"](function () {\n        _this.$message.info('已取消操作');\n      });\n    },\n    // 前往自习室详情\n    goToRoom: function goToRoom(roomId) {\n      this.$router.push(\"/student/seat-map?room=\".concat(roomId));\n    }\n  }\n};","map":{"version":3,"names":["name","data","recentReservations","id","room","seat","date","time","status","rooms","totalSeats","availableSeats","occupiedSeats","methods","getStatusType","types","handleCheckIn","reservation","$message","success","concat","handleCheckOut","handleCancel","_this","$confirm","confirmButtonText","cancelButtonText","type","then","info","goToRoom","roomId","$router","push"],"sources":["/Users/jeremiahncross/Documents/code/ymzxs/frontend/src/views/student/Dashboard.vue"],"sourcesContent":["<template>\n  <div class=\"dashboard-container\">\n    <!-- 统计卡片区 -->\n    <el-row :gutter=\"20\">\n      <el-col :xs=\"24\" :sm=\"12\" :md=\"8\" :lg=\"6\">\n        <el-card shadow=\"hover\" class=\"stat-card\">\n          <div class=\"stat-header\">\n            <div class=\"stat-title\">今日预约</div>\n            <i class=\"el-icon-date stat-icon\"></i>\n          </div>\n          <div class=\"stat-number\">2</div>\n          <div class=\"stat-footer\">\n            <span>查看详情</span>\n            <i class=\"el-icon-right\"></i>\n          </div>\n        </el-card>\n      </el-col>\n      \n      <el-col :xs=\"24\" :sm=\"12\" :md=\"8\" :lg=\"6\">\n        <el-card shadow=\"hover\" class=\"stat-card\">\n          <div class=\"stat-header\">\n            <div class=\"stat-title\">待签到</div>\n            <i class=\"el-icon-s-check stat-icon\"></i>\n          </div>\n          <div class=\"stat-number\">1</div>\n          <div class=\"stat-footer\">\n            <span>立即签到</span>\n            <i class=\"el-icon-right\"></i>\n          </div>\n        </el-card>\n      </el-col>\n      \n      <el-col :xs=\"24\" :sm=\"12\" :md=\"8\" :lg=\"6\">\n        <el-card shadow=\"hover\" class=\"stat-card\">\n          <div class=\"stat-header\">\n            <div class=\"stat-title\">本月学习时长</div>\n            <i class=\"el-icon-timer stat-icon\"></i>\n          </div>\n          <div class=\"stat-number\">24小时</div>\n          <div class=\"stat-footer\">\n            <span>查看统计</span>\n            <i class=\"el-icon-right\"></i>\n          </div>\n        </el-card>\n      </el-col>\n      \n      <el-col :xs=\"24\" :sm=\"12\" :md=\"8\" :lg=\"6\">\n        <el-card shadow=\"hover\" class=\"stat-card\">\n          <div class=\"stat-header\">\n            <div class=\"stat-title\">失信记录</div>\n            <i class=\"el-icon-warning-outline stat-icon\"></i>\n          </div>\n          <div class=\"stat-number\">0</div>\n          <div class=\"stat-footer\">\n            <span>良好信用</span>\n            <i class=\"el-icon-right\"></i>\n          </div>\n        </el-card>\n      </el-col>\n    </el-row>\n    \n    <!-- 最近预约 -->\n    <el-card class=\"recent-card\">\n      <template #header>\n        <div class=\"recent-header\">\n          <span>最近预约</span>\n          <el-button type=\"text\" @click=\"$router.push('/student/my-reservations')\">查看全部</el-button>\n        </div>\n      </template>\n      \n      <el-table :data=\"recentReservations\" stripe style=\"width: 100%\">\n        <el-table-column prop=\"room\" label=\"自习室\" width=\"180\"></el-table-column>\n        <el-table-column prop=\"seat\" label=\"座位号\" width=\"120\"></el-table-column>\n        <el-table-column prop=\"date\" label=\"日期\" width=\"180\"></el-table-column>\n        <el-table-column prop=\"time\" label=\"时间段\"></el-table-column>\n        <el-table-co